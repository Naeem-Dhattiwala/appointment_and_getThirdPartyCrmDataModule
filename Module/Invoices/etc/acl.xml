<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Iram_Invoices::Invoice" title="Invoice" sortOrder="10">
					<resource id="Iram_Invoices::Invoice_save" title="Save Invoice" sortOrder="10"/>
					<resource id="Iram_Invoices::Invoice_delete" title="Delete Invoice" sortOrder="20"/>
